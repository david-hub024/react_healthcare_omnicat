{
  "name": "OmniCAT",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "gitlab.com:crockett.jesse/omnicat.git"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production"
  },
  "dependencies": {
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-router-dom": "^4.3.1",
    "truffle-contract": "^4.0.0-next.0",
    "web3": "^1.0.0-beta.36"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "ajv": "^6.5.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0-beta.6",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^4.0.0-alpha.2",
    "node-sass": "^4.9.3",
    "openzeppelin-solidity": "^1.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "quotes": [
        1,
        "single",
        "avoid-escape"
      ],
      "prefer-const": 1,
      "react/jsx-indent": [
        1,
        2
      ],
      "no-return-assign": 1,
      "no-console": 0,
      "no-unused-vars": [
        1,
        {
          "vars": "all",
          "args": "after-used",
          "argsIgnorePattern": "^_|^next$",
          "varsIgnorePattern": "^_|^log$|^err|^info$|^warn$"
        }
      ]
    }
  }
}
